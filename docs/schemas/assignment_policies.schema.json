{
    "$schema": "http://json-schema.org/draft-04/schema#",
	"id": "/",
    "type": "object",
	"patternProperties": {
        "tag:nextthought.com,.+": {
            "additionalProperties": false,
            "type": "object",
            "properties": {
                "Title": {
                    "type": "string",
                    "id": "Title"
                },
                "available_for_submission_beginning": {
                    "type": "string",
                    "id": "available_for_submission_beginning"
                },
                "available_for_submission_ending": {
                    "type": "string",
                    "id": "available_for_submission_ending"
                },
                "student_nuclear_reset_capable": {
                    "type": "boolean",
                    "id": "student_nuclear_reset_capable"
                },
                "excluded": {
                    "type": "boolean",
                    "id": "excluded"
                },
                "auto_grade": {
                    "type": "object",
                    "id": "auto_grade",
                    "properties": {
                        "total_points": {
                            "type": "integer",
                            "id": "total_points"
                        },
                        "name": {
                            "type": "string",
                            "id": "name"
                        },
                        "questions": {
                            "default": {
                                "type": "integer",
                                "id": "default"
                            },
                            "{question-ntiid}": {
                                "type": "integer",
                                "id": "default"
                            }
                        }
                    },
					"additionalProperties": false
                }
            }
        }
    },
	"additionalProperties": false
}